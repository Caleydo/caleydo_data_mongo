{
  "name": "caleydo_data_graph",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_data_graph",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_server": "*"
  },
  "caleydo": {
    "plugins": {
      "python": [
        {
          "type": "dataset-provider",
          "id": "dataset-graph",
          "file": "graph"
        },
        {
          "type": "dataset-specific-handler",
          "id": "handler-graph",
          "file": "graph",
          "datatype": "graph",
          "factory": "add_graph_handler"
        }
      ]
    },
    "dependencies": {
      "python": {
        "py2neo": ">=2.0.4"
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
