{
  "name": "caleydo_data_mongo",
  "description": "Data provider plugin for loading graph data stored in a MongoDB.",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_data_mongo",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_server": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "python_data",
    "plugins": {
      "python": [
        {
          "type": "dataset-provider",
          "id": "dataset-graph",
          "file": "graph"
        }
      ]
    },
    "dependencies": {
      "python": {
        "pymongo": "==3.0.3"
      },
      "debian": {
        "mongodb": ""
      },
      "redhat": {
        "mongodb-org-server": ""
      }
    },
    "scripts": {
      "pre_start": "sudo service mongodb start",
      "pre_start_redhat": "sudo service mongod start"
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
